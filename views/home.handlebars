<main class="">
    <h2>Lista de cuentas</h2>

    {{#each cuentas}}
    <div class="">
        <div class="">
            <h3>{{nombre}}</h3>
        </div>
        <div class="">
            <h3>{{tipo}}</h3>
        </div>
        <div class="">
            <h3>{{monto}}</h3>
        </div>

        <div class="">
            <a href="/cuenta/{{url}}" class="">Más información</a>
        </div>
    </div>
    {{/each}}
	
</main>

